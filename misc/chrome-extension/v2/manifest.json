{
  "name": "Block",
  "description": "把特定网络 js 文件指向到本地",
  "version": "0.2",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://img.alicdn.com/tps/*"
  ],
  "browser_action": {},
  "background": {
    "scripts": ["bg.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["*://*/*", "https://www.alipay.com/*", "<all_urls>"],
      "js": ["inj.js"],
      "css": ["inject.css"],
      "all_frames": true,
      "match_about_blank": true,
      "match_origin_as_fallback": true,
      "run_at": "document_end"
    }
  ]
}
