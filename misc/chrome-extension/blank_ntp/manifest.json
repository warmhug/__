{
  "name": "Blank new tab page",
  "description": "Override the new tab page with a blank one",
  "version": "0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "permissions": [
    "webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"tabs",
		"topSites",
    "history",
    "webNavigation",
    "bookmarks"
  ],
  "background": {
    "scripts": ["bg.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["inject/util.js", "inject/inject.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "browser_action": {},
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://code.jquery.com https://gw.alipayobjects.com; object-src 'self'",
  "manifest_version": 2
}
